{
	"name": "node-ossc",
	"version": "0.1.0",
	"description": "",
	"main": "src/server.ts",
	"author": "Andr√© Kuhlmann <akuhltime@gmail.com>",
	"license": "ISC",
	"private": true,
	"engines": {
		"node": ">= 12"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"scripts": {
		"build": "npm run build-ts && npm run lint",
		"build-ts": "tsc",
		"clean": "rimraf coverage dist tmp",
		"debug": "npm run build",
		"lint": "tsc --noEmit && eslint . --ext .js,.ts,.tsx --quiet --fix",
		"lint:dev": "eslint . --ext .js,.ts,.tsx --fix",
		"serve": "node dist/server.js",
		"server-debug": "nodemon --inspect dist/server.js",
		"start": "npm run serve",
		"watch": "concurrently -k -p '[{name}]' -n 'YAML,TypeScript,Node' -c 'orange.bold,yellow.bold,cyan.bold' 'when-changed ' 'npm run watch-ts' 'npm run watch-node'",
		"watch-debug": "concurrently -k -p '[{name}]' -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'npm run serve-debug'",
		"watch-node": "nodemon dist/server.js",
		"watch-ts": "tsc -w"
	},
	"keywords": [
		"hsd",
		"ossc",
		"node",
		"express"
	],
	"devDependencies": {
		"@tsconfig/node14": "^1.0.0",
		"@types/basic-auth": "^1.1.3",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.8",
		"@types/node": "^14.11.2",
		"@types/swagger-stats": "^0.95.2",
		"@types/swagger-ui-express": "^4.1.2",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
		"colors": "^1.4.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.2",
		"rimraf": "^3.0.2",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"basic-auth": "^2.0.1",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"passport": "^0.4.1",
		"passport-http": "^0.3.0",
		"swagger-stats": "^0.95.17",
		"swagger-ui-express": "^4.1.4"
	}
}
