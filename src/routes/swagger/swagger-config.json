				},
				"year": {
					"type": "string"
				}
			}
		},
		"Student": {
			"type": "object",
			"properties": {
				"name": {
					"type": "string"
				},
				"registration": {
					"type": "number"
				},
				"birthday": {
					"type": "string"
				},
				"degree": {
					"type": "string"
				},
				"faculty": {
					"type": "string"
				},
				"address": {
					"type": "string"
