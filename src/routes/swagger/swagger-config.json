				"name": {
					"type": "string"
				},
				"passed": {
					"type": "boolean"
				},
				"creditPoints": {
					"type": "number"
				},
				"exams": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/Exam"
					}
				},
				"workExperiences": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/WorkExperience"
					}
				}
			}
		},
		"Exam": {
			"type": "object",
			"properties": {
				"grade": {
					"type": "number"
				},
				"passed": {
					"type": "boolean"
				},
				"semester": {
					"$ref": "#/definitions/Semester"
				},
				"examinationDate": {
					"type": "string"
				}
			}
		},
		"WorkExperience": {
			"type": "object",
			"properties": {
				"passed": {
					"type": "boolean"
				},
