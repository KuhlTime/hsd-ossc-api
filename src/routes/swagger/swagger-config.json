					"ossc"
				],
				"operationId": "getGrades",
				"consumes": [
					"application/json"
				],
				"produces": [
					"application/json"
				],
				"responses": {
					"200": {
						"description": "Successfull",
						"schema": {
							"type": "array",
							"$ref": "#/definitions/ModuleExtract"
