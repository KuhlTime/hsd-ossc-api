				"description": "",
				"tags": [
					"student"
				],
				"operationId": "getStudentInfo",
				"produces": [
					"application/json"
				],
				"responses": {
					"200": {
						"description": "Successfull",
						"schema": {
							"type": "object",
							"$ref": "#/definitions/Student"
						}
					},
					"401": {
						"$ref": "#/responses/UnauthorizedError"
					}
				},
				"security": [
					"ossc_auth"
				]
			}
		}
	},
	"securityDefinitions": {
		"ossc_auth": {
